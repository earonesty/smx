%set(name,rtrim)
%set(description,"Removes the whitespace from the end of a string.")
%set(syntax,"%%rtrim\(string\)")
%set(example,"-%%asc\(32\)hi%%asc\(32\)-
-%%rtrim\(%%asc\(32\)hi%%asc\(32\)\)-

Returns

- hi -
- hi-
")
%set(seealso,trim|ltrim|trimleft|trimright)
