%set(name,ltrim)
%set(description,"Removes the whitespace from the beginning of a string.")
%set(syntax,"%%ltrim\(string\)")
%set(example,"-%%asc\(32\)hi%%asc\(32\)-
-%%ltrim\(%%asc\(32\)hi%%asc\(32\)\)-

Returns

- hi -
-hi -
")
%set(seealso,trim|trimleft|trimright|rtrim)
