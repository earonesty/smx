%set(name,sql-quote)
%set(description,"Adds ' quotes around argument.  This properly quotes arguments that may contain quotes.")
%set(syntax,"%%sql-quote\(string\)
%%sqlq\(string\)")
%set(example,"%%sql-quote\(berry\) ---> 'berry'")
%set(seealso,sql)
%set(authorip,64.131.154.83)
%set(authorlogin,admin)
%set(author,)